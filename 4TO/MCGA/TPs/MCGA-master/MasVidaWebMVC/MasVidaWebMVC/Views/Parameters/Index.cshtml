@model IEnumerable<MasVidaWebMVC.Parameter>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParameterName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParameterValue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ParameterName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParameterValue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ParameterID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ParameterID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ParameterID })
        </td>
    </tr>
}

</table>
